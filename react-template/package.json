{
  "name": "react-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "env-cmd -r .env-rc.js -e development react-scripts start",
    "build:test": "env-cmd -r .env-rc.js -e test react-scripts build",
    "build:production": "env-cmd -r .env-rc.js -e production react-scripts build"
  },
  "dependencies": {
    "history": "^4.10.1",
    "ramda": "^0.26.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.0.1",
    "rxjs": "^6.5.3",
    "styled-components": "^4.4.0",
    "styled-normalize": "^8.0.6"
  },
  "devDependencies": {
    "env-cmd": "^9.0.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-sort-imports-es6-autofix": "^0.4.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
